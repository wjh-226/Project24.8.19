/* pages/3x3/3x3.wxss */
.container {
  padding: 20px;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-image: url("https://img520.com/gSbOf0.jpg");
  background-size: cover;
  background-position: center; 
  background-repeat: no-repeat
}

.title {
  font-size: 24px;
  margin-bottom: 20px;
}

image{
  width:250rpx;
  height:250rpx;
}
canvas{
  border:1rpx solid;
  width: 300px;
  height: 300px;
}

.footer {
  margin-top: 20px;
  width: 50%;
  text-align: center;
  font-size: 24px;
}

button {
  margin: 5px;
  width: 50%;
  border: none;
  background-color: #005fc5;
  color: white;
  border-radius: 10px;
}
.restart{
  margin: 5px;
  width: 100%;
  border: none;
  background-color: #005fc5;
  color: white;
  border-radius: 10px;
}